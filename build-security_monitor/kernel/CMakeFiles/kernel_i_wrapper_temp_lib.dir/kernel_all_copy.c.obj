    /* There is no guarantee that there is a new interrupt. */
    if (!(((irq)) <= maxIRQ && (irq) != irqInvalid)) {
        /* Sanity check: the slot can't hold an interrupt either. */
        do { if (!(!(((*active_irq_slot)) <= maxIRQ && (*active_irq_slot) != irqInvalid))) { _assert_fail("!IS_IRQ_VALID(*active_irq_slot)", "/host/kernel/src/arch/riscv/machine/hardware.c", 129, __func__); } } while(0);
